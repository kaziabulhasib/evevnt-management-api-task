generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int       @id @default(autoincrement())
  name           String
  email          String    @unique
  registrations  Event[]   @relation("UserRegistrations")
}

model Event {
  id             Int       @id @default(autoincrement())
  title          String
  date           DateTime
  location       String
  capacity       Int
  registrations  User[]    @relation("UserRegistrations")
}

